<?php

/**
 * Implements hock_page_build().
 */
 function tsflea_navbar_page_build(&$page) {
   if (user_access('access navbar')) {
   $module_path = drupal_get_path('module', 'tsflea_navbar');
   drupal_add_css($module_path . '/css/tsflea.navbar.css',
     [
       'group'    => CSS_THEME,
       'media'    => 'all',
       'weight'   => 1001
     ]);
   }
 }
